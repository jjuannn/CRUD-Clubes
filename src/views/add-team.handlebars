<form id="formulario" method="POST" action="/agregar-equipo" enctype="multipart/form-data">
  <div class="form-group">
    <label for="nombre">Nombre del club:</label>
    <input type="text" id="nombre" name="name" class="form-control">
  </div>
  <div class="form-group">
    <label for="tla">Abreviatura:</label>
    <input type="text" id="tla" name="tla" class="form-control">
  </div>
  <div class="form-group">
    <label for="estadio">Estadio:</label>
    <input type="text" id="estadio" name="venue" class="form-control">
  </div>
  <div class="form-group">
    <label for="direccion">Direccion:</label>
    <input type="text" id="direccion" name="address" class="form-control">
  </div>
  <div class="form-group">
    <label for="fundacion">AÃ±o de fundacion:</label>
    <input type="number" id="fundacion" name="founded" class="form-control">
  </div>
  <div class="form-group">
    <label for="numeroId">Id:</label>
    <input type="number" id="numeroId" name="id" min="1000" class="form-control">
  </div>
  <div class="form-group">
    <label for="telefono">Telefono:</label>
    <input type="text" id="telefono" name="phone" class="form-control">
  </div>
  <div class="form-group">
    <label for="website">Sitio web:</label>
    <input type="text" class="form-control" id="website" name="website">
  </div>
  <div class="form-group">
    <label for="pais">Pais:</label>
    <input type="text" class="form-control" id="pais" name="area"
  </div>
  <div class="form-group">
      <label for="imagen">Escudo:</label>
      <input type="file" class="form-control" text="Seleccionar escudo" id="imagen" name="escudo" required>
  </div>
  <button type="submit" class="btn btn-primary">Enviar</button>
</form>
